<% if current_search_session %>
  <div id="appliedParams" class="clearfix constraints-container">
    <div id="showSearchButtons" class="show-buttons" >
      <%= render 'start_over' %>
      <%= link_back_to_catalog class: 'btn btn-primary btn-show convert-to-button', "role" => "button" %>
    </div>
    <%= render 'previous_next_doc' if @search_context %>
  </div>
<% end %>

<%= render_document_main_content_partial %>

<% content_for(:sidebar) do %>
  <%= render_document_sidebar_partial @document %>
<% end %>