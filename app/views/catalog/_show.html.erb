<% metadata = [
  'description',
  'keyword',
  'origin',
  'identifier',
  'access_and_usage_rights',
  'migration_source'
] %>

<div class='item-page__metadata-wrapper'>
  <% metadata.each do | m | %>
    <%= render 'catalog/record/item_metadata', document: document, metadata: m %>
  <% end %>
</div>